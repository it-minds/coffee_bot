  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch backend (debug)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceRoot}/Web/bin/Debug/net5.0/Web.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Web",
      "stopAtEntry": false,
      "serverReadyAction": {
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {}
    }
  ]
}
